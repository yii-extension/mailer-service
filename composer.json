{
    "name": "yii-extension/mailer-service",
    "type": "library",
    "description": "Yii3 Mailer Services",
    "keywords": [
        "yii3",
        "mailer",
        "services"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|8.0",
        "yiisoft/aliases": "^1.1",
        "yiisoft/mailer": "^3.0@dev",
        "yiisoft/mailer-swiftmailer": "^3.0@dev",
        "yiisoft/yii-event": "^3.0@dev"
    },
    "require-dev": {
        "nyholm/psr7": "^1.3.0",
        "phpunit/phpunit": "^9.5",
        "roave/infection-static-analysis-plugin": "^1.6",
        "vimeo/psalm": "^4.3",
        "yiisoft/composer-config-plugin": "^1.0@dev",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\Service\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\Service\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
